{
    "name": "Grammer-Corrector",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Extention to correct grammar of sentence",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "",
            "64": "",
        },
    },
    "icons": {
        "128": "",
        "64": "",
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "permissions": ["activeTab"]
}