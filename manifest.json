{
    "name": "Grammer-Corrector",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Extention to correct grammar of sentence",
    "browser_action": {
        "default_popup": "templates/index.html",
        "default_icon": {
            "128": "logo.png"
        }
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["static/js/script.js"],
            "css": ["static/css/style.css"]
        }
    ],
    "permissions": ["activeTab"]
}